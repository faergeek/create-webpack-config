{
  "name": "@faergeek/make-webpack-config",
  "version": "0.0.0-semantically-released",
  "main": "index.js",
  "repository": "git@github.com:faergeek/make-webpack-config.git",
  "author": "Sergey Slipchenko <me@faergeek.online>",
  "license": "MIT",
  "files": [
    "*.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --list-different .",
    "release": "semantic-release"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "@faergeek",
      "@faergeek/eslint-config/node"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.14.3",
    "postcss": "^8.3.0",
    "source-map-support": "^0.5.19",
    "webpack": "^5.36.2",
    "webpack-cli": "4.4.0"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@prefresh/babel-plugin": "^0.4.1",
    "@prefresh/webpack": "^3.3.0",
    "assets-webpack-plugin": "^7.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss-loader": "^5.2.0",
    "react-refresh": "0.9.0",
    "resolve-url-loader": "^4.0.0",
    "run-script-webpack-plugin": "^0.0.11",
    "sass-loader": "^12.0.0",
    "source-map-loader": "^3.0.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-plugin-serve": "^1.4.1",
    "webpackbar": "^5.0.0-3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@faergeek/eslint-config": "^4.0.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2"
  }
}
