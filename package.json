{
  "name": "@faergeek/make-webpack-config",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:faergeek/make-webpack-config.git",
  "author": "Sergey Slipchenko <me@faergeek.online>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --list-different .",
    "release": "semantic-release"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "@faergeek",
      "@faergeek/eslint-config/node"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "webpack": "^5.36.2",
    "webpack-cli": "4.4.0"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "assets-webpack-plugin": "^7.1.0",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^1.6.0",
    "react-refresh": "^0.10.0",
    "run-script-webpack-plugin": "^0.0.11",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-plugin-serve": "^1.4.1",
    "webpackbar": "^5.0.0-3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@faergeek/eslint-config": "^1.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.25.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2"
  }
}
